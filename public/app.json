{
  "expo": {
    "scheme": "AniWise",
    "name": "AniWise",
    "slug": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Aniwise2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Aniwise2.png",
      "backgroundColor": "#ffffff", 
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Aniwise2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/Aniwise2.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
